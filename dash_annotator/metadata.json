      },
      "data_type": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Type of data passed (pdf, html, text)."
      },
      "height": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Height of annotator container.",
        "defaultValue": {
          "value": "'100vh'",
          "computed": false
        }
      },
      "width": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Width of annotator container.",
        "defaultValue": {
          "value": "'100%'",
          "computed": false
        }
      },
      "setProps": {
        "type": {
          "name": "func"
        },
        "required": false,
        "description": "Dash-assigned callback that should be called to report property changes\r\nto Dash, to make them available for callbacks."
      }
    }
  },
